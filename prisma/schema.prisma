generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model funko_tabela {
  id              Int     @id @default(autoincrement())
  nome_personagem String? @db.VarChar(60)
  nome_franquia   String? @db.VarChar(60)
  numero          Int?
  raridade        String? @db.VarChar(50)
  condicao        String? @db.VarChar(50)
  preco           Int?
  dataaquisicao   String? @db.VarChar(60)
  edicaoespecial  String? @db.VarChar(60)
}
